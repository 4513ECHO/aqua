{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/aquaproj/aqua/pkg/config/registry/config",
  "$ref": "#/$defs/Config",
  "$defs": {
    "Alias": {
      "properties": {
        "name": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "name"
      ]
    },
    "Config": {
      "properties": {
        "packages": {
          "items": {
            "$ref": "#/$defs/PackageInfo"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "packages"
      ]
    },
    "File": {
      "properties": {
        "name": {
          "type": "string"
        },
        "src": {
          "type": "string"
        },
        "dir": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "FormatOverride": {
      "properties": {
        "goos": {
          "type": "string",
          "enum": [
            "aix",
            "android",
            "darwin",
            "dragonfly",
            "freebsd",
            "illumos",
            "ios",
            "js",
            "linux",
            "netbsd",
            "openbsd",
            "plan9",
            "solaris",
            "windows"
          ]
        },
        "format": {
          "type": "string",
          "examples": [
            "tar.gz",
            "raw"
          ]
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "goos",
        "format"
      ]
    },
    "Override": {
      "properties": {
        "goos": {
          "type": "string",
          "enum": [
            "aix",
            "android",
            "darwin",
            "dragonfly",
            "freebsd",
            "illumos",
            "ios",
            "js",
            "linux",
            "netbsd",
            "openbsd",
            "plan9",
            "solaris",
            "windows"
          ]
        },
        "goarch": {
          "type": "string",
          "enum": [
            "386",
            "amd64",
            "arm",
            "arm64",
            "mips",
            "mips64",
            "mips64le",
            "mipsle",
            "ppc64",
            "ppc64le",
            "riscv64",
            "s390x",
            "wasm"
          ]
        },
        "replacements": {
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "format": {
          "type": "string",
          "examples": [
            "tar.gz",
            "raw"
          ]
        },
        "asset": {
          "type": "string"
        },
        "files": {
          "items": {
            "$ref": "#/$defs/File"
          },
          "type": "array"
        },
        "url": {
          "type": "string"
        },
        "complete_windows_ext": {
          "type": "boolean"
        },
        "windows_ext": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "PackageInfo": {
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "github_release",
            "github_content",
            "github_archive",
            "http",
            "go",
            "go_install"
          ]
        },
        "repo_owner": {
          "type": "string"
        },
        "repo_name": {
          "type": "string"
        },
        "asset": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "format": {
          "type": "string",
          "examples": [
            "tar.gz",
            "raw"
          ]
        },
        "files": {
          "items": {
            "$ref": "#/$defs/File"
          },
          "type": "array"
        },
        "url": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "link": {
          "type": "string"
        },
        "replacements": {
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "overrides": {
          "items": {
            "$ref": "#/$defs/Override"
          },
          "type": "array"
        },
        "format_overrides": {
          "items": {
            "$ref": "#/$defs/FormatOverride"
          },
          "type": "array"
        },
        "version_constraint": {
          "type": "string"
        },
        "version_overrides": {
          "items": {
            "$ref": "#/$defs/VersionOverride"
          },
          "type": "array"
        },
        "supported_if": {
          "type": "string"
        },
        "supported_envs": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "version_filter": {
          "type": "string"
        },
        "rosetta2": {
          "type": "boolean"
        },
        "aliases": {
          "items": {
            "$ref": "#/$defs/Alias"
          },
          "type": "array"
        },
        "version_source": {
          "type": "string"
        },
        "complete_windows_ext": {
          "type": "boolean"
        },
        "windows_ext": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "type"
      ]
    },
    "VersionOverride": {
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "github_release",
            "github_content",
            "github_archive",
            "http"
          ]
        },
        "repo_owner": {
          "type": "string"
        },
        "repo_name": {
          "type": "string"
        },
        "asset": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "format": {
          "type": "string",
          "examples": [
            "tar.gz",
            "raw"
          ]
        },
        "files": {
          "items": {
            "$ref": "#/$defs/File"
          },
          "type": "array"
        },
        "url": {
          "type": "string"
        },
        "replacements": {
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "overrides": {
          "items": {
            "$ref": "#/$defs/Override"
          },
          "type": "array"
        },
        "format_overrides": {
          "items": {
            "$ref": "#/$defs/FormatOverride"
          },
          "type": "array"
        },
        "supported_if": {
          "type": "string"
        },
        "supported_envs": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "version_constraint": {
          "type": "string"
        },
        "version_filter": {
          "type": "string"
        },
        "version_source": {
          "type": "string"
        },
        "rosetta2": {
          "type": "boolean"
        },
        "complete_windows_ext": {
          "type": "boolean"
        },
        "windows_ext": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    }
  }
}
