{
  automerge: true,
  extends: [
    "config:base",
    "github>suzuki-shunsuke/renovate-config:golang",
    "github>aquaproj/aqua-renovate-config",
    "github>aquaproj/aqua-renovate-config:file(aqua-global.yaml)",
    "github>aquaproj/aqua-renovate-config:file(aqua-global-2.yaml)",
    "github>aquaproj/aqua-renovate-config:file(aqua/.*\\.ya?ml)",
  ],
  packageRules: [
    {
      automerge: false,
      "matchUpdateTypes": ["major"],
    },
  ],
  regexManagers: [
    {
      fileMatch: ["README.md"],
      depNameTemplate: "aquaproj/aqua-installer",
      datasourceTemplate: "github-releases",
      matchStrings: [
        "aqua-installer/(?<currentValue>.*)/aqua-installer",
        "aqua-installer@(?<currentValue>.*)",
      ],
    },
    {
      fileMatch: ["README\\.md", "docs/.*\\.md"],
      datasourceTemplate: "github-releases",
      matchStrings: [
        '(version|ref): "?(?<currentValue>.*?)"? +# renovate: depName=(?<depName>.*?)\\n',
        '-v "?(?<currentValue>.*?)"? +# renovate: depName=(?<depName>.*?)\\n',
      ],
    },
    {
      fileMatch: [".*\\.go"],
      depNameTemplate: "aquaproj/aqua-proxy",
      datasourceTemplate: "github-releases",
      matchStrings: [
        '"(?<currentValue>.*)", // renovate: depName=aquaproj/aqua-proxy',
      ],
    },
  ],
}
