{
  automerge: true,
  extends: [
    "config:base",
    "github>suzuki-shunsuke/renovate-config:golang",
    "github>aquaproj/aqua-renovate-config",
    "github>aquaproj/aqua-renovate-config:file(aqua-global.yaml)",
    "github>aquaproj/aqua-renovate-config:file(aqua-global-2.yaml)",
    "github>aquaproj/aqua-renovate-config:file(aqua/.*\\.ya?ml)",
  ],
  packageRules: [
    {
      automerge: false,
      matchUpdateTypes: ["major"],
    },
  ],
  regexManagers: [
    {
      fileMatch: [".*\\.go"],
      depNameTemplate: "aquaproj/aqua-proxy",
      datasourceTemplate: "github-releases",
      matchStrings: [
        '"(?<currentValue>.*)", // renovate: depName=aquaproj/aqua-proxy',
      ],
    },
  ],
}
